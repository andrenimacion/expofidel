.example-container 
  width 100%
  border 1px solid rgba(0, 0, 0, 0.5)


.example-sidenav-content 
  display flex
  height 100%
  align-items center
  justify-content center


.example-sidenav
  padding 20px


.header
    display flex 

.head
    background-color #101721
    width 100%

.btn
    background #0596B0 !important
    color whitesmoke
    width 11px
    display flex
    justify-content center
    align-items center
    cursor pointer
    transition ease  all 0.5s
    border-radius 0px
    
    &:hover
        transition ease all 0.5s
        background gray !important


.dashApps
    background-color #4B668E

    
